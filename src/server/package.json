{
  "name": "pacamara-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": "percburk",
  "license": "ISC",
  "main": "dist/server.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^8.0.2",
    "cookie-session": "^2.0.0-rc.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "heroku": "^8.0.5",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0"
  },
  "scripts": {
