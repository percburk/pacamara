{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "dist",
    "rootDir": "./",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "noImplicitReturns": true,
    "noEmit": true
  },
  "include": [
    "./*.ts",
    "./*.tsx",
    "./*",
    "*",
    "./"
  ],
  "files": [
    "user.d.ts",
    "s3router.d.ts",
    "s3uploader.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
